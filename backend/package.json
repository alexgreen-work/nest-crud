{
  "name": "nest-crud",
  "version": "1.0.0",
  "description": "NestJS CRUD для товаров",
  "scripts": {
    "start": "nest start",
    "seed": "ts-node -r tsconfig-paths/register src/seeds/products.seed.ts",
    "seed:rollback": "ts-node -r tsconfig-paths/register src/seeds/rollback.seed.ts",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:dev:old": "rimraf dist",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@nestjs/common": "^11.0.10",
    "@nestjs/core": "^11.0.10",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "run-script-webpack-plugin": "^0.2.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.98.0",
    "webpack-node-externals": "^3.0.0"
  }
}
